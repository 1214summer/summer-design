{
  "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "outDir": "./dist",
      "rootDir": "./src",
      "jsx": "react-jsx",
      "allowImportingTsExtensions": true,
      "noEmit": true
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}

